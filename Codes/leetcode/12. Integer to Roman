class Solution {
public: 
    string roman(int num,string v){
       int z=num%10;
        num=num/10;
        switch(z){
            case 0:
                break;
                                case 1:
                v.push_back('I');
                break;           case 2:
                v.push_back('I');
            
               v.push_back('I');
                
                break;            case 3:
               v.push_back('I');
                
               v.push_back('I');
                
                v.push_back('I');
                
