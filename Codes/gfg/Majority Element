class Solution{
  public:
     // Function to find majority element in the array
    // a: input array
    // size: size of input array
    int majorityElement(int a[], int n)
    {
        
        // your code here
        map<int,int>m;
        for(int i=0;i<n;i++){
            m[a[i]]++;
            
            
        }
        int z=n/2;
        for(auto& it:m){
        if(it.second>z) return it.first;
            
            
        }
        
        return -1;
        
        
        
        
        
        
        
        
        
    }
};
